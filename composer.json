{
    "name": "kristenlk/oauth2-marketo",
    "description": "Marketo OAuth 2.0 support for the PHP League's OAuth2-Client",
    "keywords": [
        "oauth",
        "oauth2",
        "authorization",
        "authentication",
        "idp",
        "identity",
        "sso",
        "single sign on",
        "marketo",
        "marketo api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Kristen Kehlenbeck",
            "email": "klkehlenbeck@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "league/oauth2-client": "^1.0 | ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Kristenlk\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kristenlk\\OAuth2\\Client\\Test\\": "test/src/"
        }
    }
}
